{
    "name": "sassnowski/laravel-shareable-models",
    "description": "Create shareable links from your eloquent models.",
    "type": "library",
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/http": "^8.23.1",
        "illuminate/database": "^8.23.1",
        "illuminate/support": "^8.23.1",
        "ramsey/uuid": "^4.0"
    },
    "require-dev": {
        "doctrine/dbal": "^3.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9",
        "mockery/mockery": "^1.4",
        "nunomaduro/larastan": "^0.6.4"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Kai Sassnowski",
            "email": "me@kai-sassnowki.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Sassnowski\\LaravelShareableModel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sassnowski\\LaravelShareableModel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sassnowski\\LaravelShareableModel\\ShareableLinkServiceProvider"
            ]
        }
    }
}
